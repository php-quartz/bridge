{
    "name": "formapro/php-quartz-bridge",
    "type": "library",
    "minimum-stability": "dev",
    "description": "Job Time Scheduler Library",
    "keywords": ["job", "time", "task", "time scheduler", "quartz", "chrono"],
    "license": "MIT",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5"
    },
    "autoload": {
        "psr-4": { "Quartz\\Bridge\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
