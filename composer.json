{
    "name": "php-quartz/bridge",
    "type": "library",
    "minimum-stability": "dev",
    "description": "Job Time Scheduler Library",
    "keywords": ["job", "time", "task", "time scheduler", "quartz", "chrono"],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "symfony/framework-bundle": "^3|^4",
        "enqueue/enqueue": "^0.9",
        "queue-interop/queue-interop": "^0.7|^0.8",
        "php-quartz/quartz": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": { "Quartz\\Bridge\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
