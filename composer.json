{
    "name": "php-quartz/bridge",
    "type": "library",
    "minimum-stability": "dev",
    "description": "Job Time Scheduler Library",
    "keywords": ["job", "time", "task", "time scheduler", "quartz", "chrono"],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^2.8|^3.0",
        "enqueue/enqueue": "^0.7@dev|^0.8@dev",
        "queue-interop/queue-interop": "^0.6@dev",
        "php-quartz/quartz": "^0.1@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5"
    },
    "autoload": {
        "psr-4": { "Quartz\\Bridge\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
